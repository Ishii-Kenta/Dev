<Page
    x:Class="NavigationSample.Views.AirControllerView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:NavigationSample.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:NavigationSample.ViewModels"
    Title="AirControllerView"
    d:DataContext="{d:DesignInstance {x:Type viewModels:AirControllerViewModel}}"
    d:DesignHeight="300"
    d:DesignWidth="300"
    Background="Orange"
    mc:Ignorable="d">

    <Grid>
        <StackPanel HorizontalAlignment="Right" VerticalAlignment="Top">
            <Button Command="{Binding PrevCommand, Mode=OneWay}" Content="Prev" />
            <Button Command="{Binding NextCommand, Mode=OneWay}" Content="Next" />
        </StackPanel>
    </Grid>
</Page>
